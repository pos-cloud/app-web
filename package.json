{
  "name": "pos-cloud",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:prod": "ng build --configuration production",
    "build:prodp": "ng build --configuration production --output-path=dist/prod --base-href=/system/prod/",
    "build:dev": "ng build --configuration dev",
    "test:headless": "ng test --watch=false --browsers=ChromeHeadless",
    "tienda-nube": "cd services/tienda-nube && npm run start:dev",
    "api-v1": "cd services/api-v1 && npm run dev",
    "api-v2": "cd services/api-v2 && npm run dev",
    "print": "cd services/print && npm run dev",
    "storage": "cd services/storage && npm run start:dev",
    "socket" : "cd services/socket && node index.js",
    "dev": "concurrently -n 'Angular,APIv1,APIv2,Storage,TiendaNube,Print' -c 'red,blue,yellow,magenta,cyan,green' \"ng serve\" \"npm run api-v1\" \"npm run api-v2\" \"npm run storage\" \"npm run tienda-nube\" \"npm run print\""
  },
